# Gravix Environment Configuration
# Copy this file to .env and fill in your values:
#   cp .env.example .env

# Ingestion API key (required for authenticated ingestion)
API_KEY=change-me-to-a-real-secret

# MinIO / S3 Storage
S3_ENDPOINT=http://minio:9000
S3_REGION=us-east-1
S3_BUCKET=gravix
S3_ACCESS_KEY=admin
S3_SECRET_KEY=change-me-to-a-real-secret

# MinIO Root Credentials
MINIO_ROOT_USER=admin
MINIO_ROOT_PASSWORD=change-me-to-a-real-secret

# Cube.js API Secret (optional â€” leave empty to disable auth on dashboard queries)
CUBEJS_API_SECRET=
